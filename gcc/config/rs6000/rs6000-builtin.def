/* Before including this file, the RS6000_BUILTIN macro must be defined with the following arguments:

   ENUM		-- Enumeration name.
   NAME		-- String literal of the function name.
   ICODE	-- Insn code of the rtl expander to create the function.
   TYPE		-- Builtin type (altivec, vsx, etc.).
   BTC		-- Classify the builtin attributes (pure, saturation, memory).
   RET		-- Builtin return type.
   ARG1		-- First argument type.
   ARG2		-- Second argument type.
   ARG3		-- Third argument type.  */

#ifndef RS6000_OPTS_H
#include "rs6000-opts.h"
#endif

#ifndef RS6000_BUILTIN_OLD
#define RS6000_BUILTIN_OLD(ENUM, BTC)					\
  RS6000_BUILTIN (ENUM,			/* enum name.  */		\
		  NULL,			/* builtin name.  */		\
		  CODE_FOR_nothing,	/* function code.  */		\
		  RS6000_BUILTIN_NONE,	/* type of builtin.  */		\
		  BTC,			/* classification.  */		\
		  VOIDmode,		/* return type.  */		\
		  VOIDmode,		/* first argument type.  */	\
		  VOIDmode,		/* second argument type.  */	\
		  VOIDmode)		/* third argument type.  */

/* Helper macros that assume all of the arguments are the same mode.

   Most of the builtins that take vector integer are const (only depend on the
   arguments and do not read memory), unless they do saturation under Altivec,
   which sets the SAT register.

   For the floating point builtins, if rounding mode is on, then treat the
   function as not reading global memory, but it can have arbitrary side
   effects.  If it is off, then assume the function is a const function.  This
   mimics the ATTR_MATHFN_FPROUNDING attribute in builtin-attribute.def that is
   used for the math functions.  Comparisons don't depend on the rounding
   mode.  */

#define RS6000_BU_INT(ENUM, NAME, ICODE, TYPE, NUM, MODE)		\
  RS6000_BUILTIN (ENUM, NAME, ICODE, TYPE, RS6000_BTC_CONST, MODE,	\
		  ((NUM) >= 1) ? MODE : VOIDmode,			\
		  ((NUM) >= 2) ? MODE : VOIDmode,			\
		  ((NUM) >= 3) ? MODE : VOIDmode)

#define RS6000_BU_PURE(ENUM, NAME, ICODE, TYPE, NUM, MODE)		\
  RS6000_BUILTIN (ENUM, NAME, ICODE, TYPE, RS6000_BTC_PURE, MODE,	\
		  ((NUM) >= 1) ? MODE : VOIDmode,			\
		  ((NUM) >= 2) ? MODE : VOIDmode,			\
		  ((NUM) >= 3) ? MODE : VOIDmode)

#define RS6000_BU_SAT(ENUM, NAME, ICODE, TYPE, NUM, MODE)		\
  RS6000_BUILTIN (ENUM, NAME, ICODE, TYPE, RS6000_BTC_MISC, MODE,	\
		  ((NUM) >= 1) ? MODE : VOIDmode,			\
		  ((NUM) >= 2) ? MODE : VOIDmode,			\
		  ((NUM) >= 3) ? MODE : VOIDmode)

#define RS6000_BU_FP(ENUM, NAME, ICODE, TYPE, NUM, MODE)		\
  RS6000_BUILTIN (ENUM, NAME, ICODE, TYPE, RS6000_BTC_FP_PURE, MODE,	\
		  ((NUM) >= 1) ? MODE : VOIDmode,			\
		  ((NUM) >= 2) ? MODE : VOIDmode,			\
		  ((NUM) >= 3) ? MODE : VOIDmode)

#define RS6000_BU_FPC(ENUM, NAME, ICODE, TYPE, NUM, MODE)		\
  RS6000_BUILTIN (ENUM, NAME, ICODE, TYPE, RS6000_BTC_CONST, MODE,	\
		  ((NUM) >= 1) ? MODE : VOIDmode,			\
		  ((NUM) >= 2) ? MODE : VOIDmode,			\
		  ((NUM) >= 3) ? MODE : VOIDmode)


