/* Before including this file, the BU_RS6000 macro must be defined with the following arguments:

   ENUM		-- Enumeration name.
   NAME		-- String literal of the function name.
   ICODE	-- Insn code of the rtl expander to create the function.
   TYPE		-- Builtin type (altivec, vsx, etc.).
   BTC		-- Classify the builtin attributes (pure, saturation, memory).
   RET		-- Builtin return type.
   ARG1		-- First argument type.
   ARG2		-- Second argument type.
   ARG3		-- Third argument type.  */

#ifndef RS6000_BUILTIN
/* old macro.  */
#define RS6000_BUILTIN(ENUM, BTC)					\
  BU_RS6000 (ENUM,			/* enum name.  */		\
	     NULL,			/* builtin name.  */		\
	     CODE_FOR_nothing,		/* function code.  */		\
	     RS6000_BUILTIN_NONE,	/* type of builtin.  */		\
	     BTC,			/* classification.  */		\
	     VOIDmode,			/* return type.  */		\
	     VOIDmode,			/* first argument type.  */	\
	     VOIDmode,			/* second argument type.  */	\
	     VOIDmode)			/* third argument type.  */

/* Helper macros using token pasting to keep things to a single line.

   Most of the builtins that take vector integer are const (only depend on the
   arguments and do not read memory), unless they do saturation under Altivec,
   which sets the SAT register.

   For the floating point arithmetic builtins, if rounding mode is on, then
   treat the function as not reading global memory, but it can have arbitrary
   side effects.  If it is off, then assume the function is a const function.
   This mimics the ATTR_MATHFN_FPROUNDING attribute in builtin-attribute.def
   that is used for the math functions.  Comparisons don't depend on the
   rounding mode.  Floating point comparisons/min/max can be const since the
   rounding mode doesn't affect things.  */

/* Altivec builtins that have 1 input and 1 output.  */
#define BU_AV1(ENUM, NAME, ICODE, BTC, RET, ARG1)			\
  BU_RS6000 (ALTIVEC_BUILTIN_ ## ENUM,			/* ENUM */	\
	     "__builtin_altivec_" NAME,			/* NAME */	\
	     CODE_FOR_ ## ICODE,			/* ICODE */	\
	     RS6000_BUILTIN_ALTIVEC,			/* TYPE */	\
	     RS6000_BTC_ ## BTC,			/* BTC */	\
	     RET ## mode,				/* RET */	\
	     ARG1 ## mode,				/* ARG1 */	\
	     VOIDmode,					/* ARG2 */	\
	     VOIDmode)					/* ARG3 */

/* Altivec builtins that have 2 inputs and 1 output.  */
#define BU_AV2(ENUM, NAME, ICODE, BTC, RET, ARG1, ARG2)			\
  BU_RS6000 (ALTIVEC_BUILTIN_ ## ENUM,			/* ENUM */	\
	     "__builtin_altivec_" NAME,			/* NAME */	\
	     CODE_FOR_ ## ICODE,			/* ICODE */	\
	     RS6000_BUILTIN_ALTIVEC,			/* TYPE */	\
	     RS6000_BTC_ ## BTC,			/* BTC */	\
	     RET ## mode,				/* RET */	\
	     ARG1 ## mode,				/* ARG1 */	\
	     ARG2 ## mode,				/* ARG2 */	\
	     VOIDmode)					/* ARG3 */

/* Altivec builtins that have 3 inputs and 1 output.  */
#define BU_AV3(ENUM, NAME, ICODE, BTC, RET, ARG1, ARG2, ARG3)		\
  BU_RS6000 (ALTIVEC_BUILTIN_ ## ENUM,			/* ENUM */	\
	     "__builtin_altivec_" NAME,			/* NAME */	\
	     CODE_FOR_ ## ICODE,			/* ICODE */	\
	     RS6000_BUILTIN_ALTIVEC,			/* TYPE */	\
	     RS6000_BTC_ ## BTC,			/* BTC */	\
	     RET ## mode,				/* RET */	\
	     ARG1 ## mode,				/* ARG1 */	\
	     ARG2 ## mode,				/* ARG2 */	\
	     ARG3 ## mode)				/* ARG3 */

/* Altivec builtins that have 3 inputs (all same type) and 1 ouptut.  */
#define BU_AV3X(ENUM, NAME, ICODE, BTC, MODE)				\
  BU_RS6000 (ALTIVEC_BUILTIN_ ## ENUM,			/* ENUM */	\
	     "__builtin_altivec_" NAME,			/* NAME */	\
	     CODE_FOR_ ## ICODE,			/* ICODE */	\
	     RS6000_BUILTIN_ALTIVEC,			/* TYPE */	\
	     RS6000_BTC_ ## BTC,			/* BTC */	\
	     MODE ## mode,				/* RET */	\
	     MODE ## mode,				/* ARG1 */	\
	     MODE ## mode,				/* ARG2 */	\
	     MODE ## mode)				/* ARG3 */

/* Altivec unsigned multiply variants.  */
#define BU_AV2U(ENUM, NAME, ICODE, BTC, RET, ARG1, ARG2)		\
  BU_RS6000 (ALTIVEC_BUILTIN_ ## ENUM ## _UNS,		/* ENUM */	\
	     "__builtin_altivec_" NAME "_uns",		/* NAME */	\
	     CODE_FOR_ ## ICODE,			/* ICODE */	\
	     RS6000_BUILTIN_ALTIVEC,			/* TYPE */	\
	     RS6000_BTC_ ## BTC,			/* BTC */	\
	     RET ## mode,				/* RET */	\
	     ARG1 ## mode,				/* ARG1 */	\
	     ARG2 ## mode,				/* ARG2 */	\
	     VOIDmode)					/* ARG3 */

/* Altivec permute builtins that have 3 inputs and 1 ouptut, with the output
   and the first 2 inputs having the same type, and the 3rd input being
   V16QImode.  */
#define BU_AV3P(ENUM, NAME, ICODE, BTC, MODE)				\
  BU_RS6000 (ALTIVEC_BUILTIN_ ## ENUM,			/* ENUM */	\
	     "__builtin_altivec_" NAME,			/* NAME */	\
	     CODE_FOR_ ## ICODE,			/* ICODE */	\
	     RS6000_BUILTIN_ALTIVEC,			/* TYPE */	\
	     RS6000_BTC_ ## BTC,			/* BTC */	\
	     MODE ## mode,				/* RET */	\
	     MODE ## mode,				/* ARG1 */	\
	     MODE ## mode,				/* ARG2 */	\
	     V16QImode)					/* ARG3 */

/* Altivec vsldoi builtins that have 3 inputs and 1 ouptut, with the output
   and the first 2 inputs having the same type, and the 3rd input being
   QImode.  */
#define BU_AV3V(ENUM, NAME, ICODE, BTC, MODE)				\
