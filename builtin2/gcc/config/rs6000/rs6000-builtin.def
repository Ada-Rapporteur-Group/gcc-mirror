/* General paired builtins.  */
#define BU_PAIRED(ENUM, NAME, ICODE, BTC, RET, NUM, ARG)		\
  BU_RS6000 (PAIRED_BUILTIN_ ## ENUM,			/* ENUM */	\
	     "__builtin_paired_" NAME,			/* NAME */	\
	     CODE_FOR_ ## ICODE,			/* ICODE */	\
	     RS6000_BUILTIN_PAIRED,			/* TYPE */	\
	     RS6000_BTC_ ## BTC,			/* BTC */	\
	     RET ## mode,				/* RET */	\
	     (((NUM) >= 1) ? ARG ## mode : VOIDmode),	/* ARG1 */	\
	     (((NUM) >= 2) ? ARG ## mode : VOIDmode),	/* ARG2 */	\
	     (((NUM) >= 3) ? ARG ## mode : VOIDmode))	/* ARG3 */

/* General SPE builtins.  */
#define BU_SPE(ENUM, NAME, ICODE, BTC, RET, NUM, ARG)			\
  BU_RS6000 (SPE_BUILTIN_ ## ENUM,			/* ENUM */	\
	     "__builtin_spe_" NAME,			/* NAME */	\
	     CODE_FOR_ ## ICODE,			/* ICODE */	\
	     RS6000_BUILTIN_SPE,			/* TYPE */	\
	     RS6000_BTC_ ## BTC,			/* BTC */	\
	     RET ## mode,				/* RET */	\
	     (((NUM) >= 1) ? ARG ## mode : VOIDmode),	/* ARG1 */	\
	     (((NUM) >= 2) ? ARG ## mode : VOIDmode),	/* ARG2 */	\
	     (((NUM) >= 3) ? ARG ## mode : VOIDmode))	/* ARG3 */
#endif

